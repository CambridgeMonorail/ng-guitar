{
  "name": "ng-guitar",
  "packageManager": "npm@6.x",
  "version": "0.0.0",
  "description": "Angular UI components for building applications for guitarists.",
  "license": "MIT",
  "scripts": {
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected": "nx affected",
    "build": "ng build --base-href=/ng-guitar/",
    "dep-graph": "nx dep-graph",
    "docs": "node tools/scripts/generate-docs.js",
    "e2e": "ng e2e",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "format": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && ng lint",
    "ng": "nx",
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "pre-commit": "lint-staged",
    "start": "ng serve",
    "test": "nx run-many --target=test --all --parallel",
    "update:check": "ng update",
    "update": "ng update @nrwl/workspace",
    "watch": "ng build --watch --configuration development",
    "workspace-schematic": "nx workspace-schematic",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.6",
    "@angular/core": "~14.0.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "~14.0.0",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@nrwl/angular": "14.3.6",
    "@qiwi/semantic-release-gh-pages-plugin": "^5.2.3",
    "angular-audio-context": "^32.0.10",
    "angular-material-rail-drawer": "^1.0.0",
    "rxjs": "~7.4.0",
    "standardized-audio-context-mock": "^9.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.0",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "~14.0.0",
    "@angular/language-service": "~14.0.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jscutlery/semver": "^2.25.2",
    "@nrwl/cli": "14.3.6",
    "@nrwl/cypress": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.4.3",
    "@nrwl/linter": "14.3.6",
    "@nrwl/workspace": "14.3.6",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^8.0.0",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "lint-staged": "^13.0.3",
    "markdown-magic": "^2.6.0",
    "ng-packagr": "~14.0.3",
    "nx": "14.3.6",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimDMorris/ng-guitar.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@qiwi/semantic-release-gh-pages-plugin",
        {
          "src": "dist/apps/demo"
        }
      ]
    ]
  }
}
