{
  "projectType": "library",
  "sourceRoot": "libs/metronome/src",
  "prefix": "ng-guitar",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/metronome"],
      "options": {
        "project": "libs/metronome/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/metronome/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/metronome/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/metronome"],
      "options": {
        "jestConfig": "libs/metronome/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/metronome/**/*.ts",
          "libs/metronome/**/*.html"
        ]
      }
    }
  },
  "tags": ["ui"]
}
